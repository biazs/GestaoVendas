@model GestaoVendas.Models.PerfilUsuario

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Perfil Usuário</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TipoUsuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoUsuario.NomeTipoUsuario)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ItentityUser.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ItentityUser.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ItentityUser)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ItentityUser.Id)
        </dd>
    </dl>
</div>
<div class="col-sm-12">
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-block button-save">Alterar</a>
    <a asp-action="Index" class="btn btn-block button-list">Listagem de Perfil de usuários</a>
</div>
