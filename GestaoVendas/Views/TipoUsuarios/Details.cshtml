@model GestaoVendas.Models.TipoUsuario

@{
    ViewData["Title"] = "Detalhes";
}

<h1 class="titulo text-info">Detalhes</h1>

<div class="col-sm-12">
    <h4 class="titulo text-info" style="font-size: 20px;">Tipo Usuário</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomeTipoUsuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeTipoUsuario)
        </dd>
    </dl>
</div>
<div class="col-sm-12">
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-block button-save">Alterar</a>
    <a asp-action="Index" class="btn btn-block button-list">Listagem de tipos de usuários</a>
</div>
